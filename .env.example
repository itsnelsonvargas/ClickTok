# ClickTok Environment Variables (Example)
# Copy this file to .env and fill in your actual values

# TikTok Credentials
TIKTOK_USERNAME=your_tiktok_username
TIKTOK_PASSWORD=your_tiktok_password

# OpenAI API (Optional - for AI-powered captions)
OPENAI_API_KEY=sk-proj-...

# Anthropic API (Optional - alternative to OpenAI)
ANTHROPIC_API_KEY=sk-ant-...

# ElevenLabs API (Optional - for voiceovers)
ELEVENLABS_API_KEY=...

# TikTok Shop API (Optional - for real product data)
TIKTOK_SHOP_APP_KEY=...
TIKTOK_SHOP_APP_SECRET=...
TIKTOK_SHOP_ACCESS_TOKEN=...

# Application Settings
DEBUG=False
LOG_LEVEL=INFO
MAX_POSTS_PER_DAY=5
